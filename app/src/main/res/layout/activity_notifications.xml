<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.saimo.insight.Notifications"
    android:background="@drawable/notifications">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EXAMS"
        android:id="@+id/button30"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PLACEMENTS"
        android:id="@+id/button34"
        android:layout_below="@+id/button30"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PAPER PRESENTATIONS"
        android:id="@+id/button35"
        android:layout_below="@+id/button34"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JNTUK "
        android:id="@+id/button36"
        android:layout_below="@+id/button35"
        android:layout_alignStart="@+id/button30" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GATE "
        android:id="@+id/button37"
        android:layout_alignTop="@+id/button38"
        android:layout_alignStart="@+id/button34" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CAT"
        android:id="@+id/button38"
        android:layout_marginBottom="78dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BACK"
        android:id="@+id/button39"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/button35" />
</RelativeLayout>
